            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "AddWalkDto": {
        "required": [
          "description",
          "difficultyId",
          "lengthInKm",
          "name",
          "regionId",
          "walkImageUrl"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 100,
            "minLength": 5,
            "type": "string"
          },
          "description": {
            "maxLength": 30,
            "minLength": 7,
            "type": "string"
          },
          "lengthInKm": {
            "type": "number",
            "format": "double"
          },
          "walkImageUrl": {
            "minLength": 1,
            "type": "string"
          },
          "difficultyId": {
            "type": "integer",
            "format": "int32"
          },
          "regionId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
